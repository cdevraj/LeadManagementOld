<br>
<div class="container">
<div class="widget stacked widget-table action-table">

  <div class="widget-header">
    <i class="icon-th-list"></i>
    <h3>Listing Leads </h3>
  </div> <!-- /widget-header -->

  <div class="widget-content">

    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email1</th>
          <th>Email2</th>
          <th>Phone Number 1</th>
          <th>Phone Number 2</th>
          <th>Category</th>
          <th>Company Url</th>
          <th>About Me</th>
          <th>Status</th>
          <th>Comment</th>
          <th>Owner</th> 
        </tr>
      </thead>

      <tbody>
        <% @lead_generators_leads.each do |lead| %>
        <tr>
          <td> <%= lead.first_name %> </td>
          <td> <%= lead.last_name %> </td>
          <td> <%= lead.email %> </td>
          <td> <%= lead.email2 %> </td>
          <td> <%= lead.phone_number %> </td>
          <td> <%= lead.phone_number2 %> </td>
          <td> <%= lead.category %> </td>
          <td> <%= lead.company_url %> </td>
          <td> <%= lead.about_me %> </td>
          <td> <%= lead.status %> </td>
          <td> <%= lead.comment %> </td>
          <td><b><%= lead.lead_generator.name %></b></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

</div>